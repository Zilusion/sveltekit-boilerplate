{
	"name": "ui-kit",
	"version": "0.0.1",
	"author": "Sudorgin Daniil (https://github.com/Zilusion)",
	"scripts": {
		"dev": "svelte-kit sync && vite dev",
		"build": "svelte-kit sync && vite build && npm run prepack",
		"preview": "vite preview",
		"prepare": "husky",
		"prepack": "svelte-kit sync && svelte-package && publint",
		"check:types": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:format": "prettier --check .",
		"check:lint": "eslint .",
		"check:styles": "stylelint 'src/**/*.{css,scss}'",
		"check": "npm run check:types && npm run check:format && npm run check:lint",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"fix:format": "prettier --write .",
		"fix:lint": "eslint --fix .",
		"fix:styles": "stylelint 'src/**/*.{css,scss}' --fix",
		"fix": "npm run fix:lint && npm run fix:format",
		"test:unit": "vitest",
		"test": "npm run test:unit -- --run",
		"storybook": "storybook dev -p 6006",
		"build-storybook": "storybook build",
		"icons:generate": "node scripts/generate-icons.ts"
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"sideEffects": [
		"**/*.css"
	],
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		}
	},
	"peerDependencies": {
		"svelte": "^5.0.0"
	},
	"devDependencies": {
		"@chromatic-com/storybook": "4.0.1",
		"@commitlint/cli": "^19.8.1",
		"@commitlint/config-conventional": "^19.8.1",
		"@eslint/compat": "^1.2.5",
		"@eslint/js": "^9.18.0",
		"@storybook/addon-a11y": "0.0.0-pr-32020-sha-ee8b2677",
		"@storybook/addon-docs": "0.0.0-pr-32020-sha-ee8b2677",
		"@storybook/addon-svelte-csf": "5.0.7",
		"@storybook/addon-vitest": "0.0.0-pr-32020-sha-ee8b2677",
		"@storybook/sveltekit": "0.0.0-pr-32020-sha-ee8b2677",
		"@sveltejs/adapter-static": "^3.0.8",
		"@sveltejs/kit": "^2.22.0",
		"@sveltejs/package": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^6.0.0",
		"@tailwindcss/forms": "^0.5.9",
		"@tailwindcss/typography": "^0.5.15",
		"@tailwindcss/vite": "^4.0.0",
		"@types/node": "^24.1.0",
		"@vitest/browser": "^3.2.3",
		"@vitest/coverage-v8": "3.2.4",
		"cheerio": "^1.1.2",
		"eslint": "^9.18.0",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-storybook": "0.0.0-pr-32020-sha-ee8b2677",
		"eslint-plugin-svelte": "^3.0.0",
		"globals": "^16.0.0",
		"husky": "^9.1.7",
		"mdsvex": "^0.12.3",
		"playwright": "^1.53.0",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.3",
		"prettier-plugin-tailwindcss": "^0.6.11",
		"publint": "^0.3.2",
		"storybook": "0.0.0-pr-32020-sha-ee8b2677",
		"stylelint": "^16.23.0",
		"stylelint-config-standard": "^39.0.0",
		"stylelint-config-tailwindcss": "^1.0.0",
		"stylelint-prettier": "^5.0.3",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"tailwindcss": "^4.0.0",
		"ts-node": "^10.9.2",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.20.0",
		"vite": "^7.0.4",
		"vitest": "^3.2.3",
		"vitest-browser-svelte": "^0.1.0"
	},
	"keywords": [
		"svelte"
	],
	"npm": {
		"onlyBuiltDependencies": [
			"esbuild"
		]
	},
	"dependencies": {
		"class-variance-authority": "^0.7.1",
		"clsx": "^2.1.1",
		"lint-staged": "^16.1.2",
		"tailwind-merge": "^3.3.1"
	}
}
